<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NYMag Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10.0"/>
    <parameter name="browserName" value="chrome"/>
    <!--    <parameter name="browserVersion" value="79.0"/>-->
    <parameter name="url" value="https://www.nymag.com/"/>

    <test name = "NYMag Home">
        <classes>
            <class name="tests.NYMagTest"/>
            <class name="tests.NYMagTest2"/>
            <class name="tests.NYMagTest3"/>
            <class name="tests.NYMagTest4"/>
            <class name="tests.NYMagTest5"/>
            <class name="tests.NYMagTest6"/>
            <class name="tests.NYMagTest7"/>
            <class name="tests.NYMagTest8"/>
        </classes>
    </test>

</suite>